{
  "name": "typekev-bot",
  "version": "1.0.0",
  "description": "An autonomous assistant for users of my site",
  "author": "Kevin Gonzalez <kev@typekev.com>",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "watch": "nodemon ./build/index.js",
    "build": "cross-env rimraf ./build/** && babel src --out-dir build --copy-files",
    "build:prod": "babel src --out-dir . --copy-files",
    "coveralls": "npm test -- --coverage --watchAll=false && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src",
    "test": "jest src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typekev/typekev-bot.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@microsoft/recognizers-text-data-types-timex-expression": "^1.3.0",
    "botbuilder": "^4.22.2",
    "botbuilder-ai": "^4.22.2",
    "botbuilder-dialogs": "^4.22.2",
    "botbuilder-testing": "^4.22.2",
    "dotenv": "^10.0.0",
    "restify": "^8.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^3.1.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.5.1",
    "prettier-eslint-cli": "^8.0.1",
    "rimraf": "^3.0.2"
  }
}
